<div class="main">
    <div class="header">
        <div class="container">
            <h1>Welcome to MisQuote!</h1>
            <h2>Learn about all the lines you've (probably) been saying wrong.</h2>
            <p class="btn btn-primary" id="new-quote-button">New Quote</p>
        </div>
    </div>
    <div class="quote">
        <% @quotes.each do |quote| %>
        <h2 class="description"><%= quote.description %></h2>
        <h3 class="quote-text">False Quote: <br>"<%= quote.false_quote %>"</h3>
        <h3 class="quote-text">True Quote: <br>"<%= quote.true_quote %>"</h3>
        <% end %>
    </div>
</div>